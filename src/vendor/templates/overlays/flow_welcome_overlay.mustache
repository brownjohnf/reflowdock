<section class='content clearfix'>
  <div class='image not-found'></div>
  <h3 class='stripe-title'>
    Welcome to {{flowName}}
  </h3>
  {{#description}}
  <p>
    {{description}}
  </p>
  {{/description}}
  <p>
    This flow has {{flowMembersCount}} members. {{accessModeCopy}}
  </p>
  <p class="avatar-strip">
  </p>
  <hr>
  <p />
  <h5 class='stripe-title'>
    Do you want to receive @team notifications in this flow?
  </h5>
  <a class='button primary-button join-team'>Yes, I'm part of @team</a>
  <a class='button hang-around'>No thanks, I'm just spectating</a>
</section>
